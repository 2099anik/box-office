{
  "name": "my-app",
  "version": "0.1.0",
  "description": "The default template project for [nano-react-app](https://github.com/nano-react-app/nano-react-app).",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "npm run clear && vite build",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:lint": "npm run format && npm run lint",
    "clear": "rimraf dist .parcel-cache"
  },
  "dependencies": {
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "babel-preset-nano-react-app": "^0.1.0",
    "sass": "^1.56.1",
    "vite": "^3.2.1"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        },
        "yt1"
      ],
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        },
        "yt2"
      ]
    ]
  },
  "main": "index.js",
  "author": ""
}
